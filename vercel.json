{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/discord",
      "destination": "https://discord.com"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "object-src 'none'; script-src 'self' sa.igna.ooo; base-uri 'none';"
        }
      ]
    }
  ]
}
